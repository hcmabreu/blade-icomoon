{
    "version": "1.0.5",
    "name": "nerdroid23/blade-icomoon",
    "description": "A package to easily make use of \"IcoMoon\" in your Laravel Blade views.",
    "keywords": [
        "laravel",
        "blade",
        "icomoon"
    ],
    "homepage": "https://github.com/nerdroid23/blade-icomoon",
    "license": "MIT",
    "authors": [
        {
            "name": "Joe Sylnice",
            "email": "nerdroid23@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",",
        "spatie/laravel-package-tools": "*",
        "blade-ui-kit/blade-icons": "*",
        "illuminate/support": "*"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Nerdroid23\\BladeIcomoon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nerdroid23\\BladeIcomoon\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nerdroid23\\BladeIcomoon\\BladeIcomoonServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
